{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "pnpm build",
  "outputDirectory": "dist",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/example/:path*",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api/)(?!assets/)(?!example/).*)",
      "destination": "/index.html"
    }
  ]
}